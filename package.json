{
  "name": "react-checkbox",
  "version": "0.1.3",
  "description": "React Checkbox",
  "main": "lib/index.js",
  "scripts": {
    "test": "make",
    "lib": "gulp",
    "build-style": "webpack --progress --colors --config build-style.config.js",
    "dist": "webpack --progress --colors --config dist.config.js",
    "dist.min": "webpack --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe --config dist.min.config.js",
    "build": "npm run lib && npm run dist && npm run dist.min && npm run build-style",
    "serve": "http-server -p 9091",
    "dev": "webpack-dev-server --progress --colors --port 9090 --content-base http://localhost:9091"
  },
  "devDependencies": {
    "css-loader": "^0.9.0",
    "extract-text-webpack-plugin": "^0.3.5",
    "gulp": "^3.8.10",
    "gulp-react": "^2.0.0",
    "http-server": "^0.7.3",
    "jsx-loader": "^0.12.2",
    "style-loader": "^0.8.2",
    "stylus-loader": "^0.4.0",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "dependencies": {
    "object-assign": "^2.0.0",
    "react": "^0.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radubrehar/react-checkbox.git"
  },
  "keywords": [
    "checkbox",
    "check",
    "checked",
    "react",
    "react-component"
  ],
  "author": "Radu Brehar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radubrehar/react-checkbox/issues"
  },
  "homepage": "https://github.com/radubrehar/react-checkbox"
}
